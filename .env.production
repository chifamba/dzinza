# Dzinza Genealogy Platform - Production Environment Configuration

# Application Environment
NODE_ENV=production
PORT=5173

# API Configuration
VITE_API_BASE_URL=https://api.dzinza.com
VITE_AUTH_SERVICE_URL=https://auth.dzinza.com
VITE_GENEALOGY_SERVICE_URL=https://genealogy.dzinza.com
VITE_SEARCH_SERVICE_URL=https://search.dzinza.com
VITE_STORAGE_SERVICE_URL=https://storage.dzinza.com

# Authentication Configuration
VITE_JWT_STORAGE_KEY=dzinza_access_token
VITE_REFRESH_TOKEN_KEY=dzinza_refresh_token
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ENABLE_MFA=true
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_EMAIL_VERIFICATION=true

# Application Configuration
VITE_APP_NAME="Dzinza Genealogy Platform"
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_LANGUAGE=en
VITE_SUPPORTED_LANGUAGES=en,sn,nd

# Development Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# External Services (Production)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
