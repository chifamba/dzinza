# =================================================================
# PRODUCTION ENVIRONMENT CONFIGURATION
# =================================================================
# This file contains production-specific overrides
# All other configuration is inherited from the main .env file

# Application Environment
NODE_ENV=production
PORT=5173

# Production URLs
VITE_API_BASE_URL=https://api.dzinza.com
VITE_AUTH_SERVICE_URL=https://auth.dzinza.com
VITE_GENEALOGY_SERVICE_URL=https://genealogy.dzinza.com
VITE_SEARCH_SERVICE_URL=https://search.dzinza.com
VITE_STORAGE_SERVICE_URL=https://storage.dzinza.com

# Production-specific Frontend Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error
VITE_ENABLE_SOCIAL_LOGIN=true

# Production-specific Feature Flags
ENABLE_DEBUG_ROUTES=false
ENABLE_SEED_DATA=false

# Production-specific Logging
LOG_LEVEL=error

# Production-specific CORS (restrictive)
ALLOWED_ORIGINS=https://dzinza.com,https://www.dzinza.com

# External Services (Production - to be configured)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
